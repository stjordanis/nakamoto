{
  "name": "nakamoto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-runner": "jest",
    "clean": "cd android && ./gradlew clean",
    "bundle": "cross-env NODE_ENV=dev react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --reset-cache",
    "bundle:prod": "cross-env NODE_ENV=prod react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ --reset-cache",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --variant=release",
    "log": "react-native log-android"
  },
